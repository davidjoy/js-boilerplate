{
  "name": "js-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for JavaScript web applications.",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test mocha --require ./test/test-init --compilers js:babel/register --recursive",
    "test:cov": "NODE_ENV=test babel-node $(npm bin)/isparta cover $(npm bin)/_mocha -- --require ./test/test-init --recursive",
    "start": "NODE_ENV=development node server",
    "build:start": "NODE_ENV=production node server",
    "build": "rimraf dist && NODE_ENV=production webpack --config ./webpack.config.prod.js --progress --profile --colors"
  },
  "author": "David Joy",
  "license": "MIT",
  "dependencies": {
    "express": "^4.13.4",
    "history": "^1.17.0",
    "isomorphic-fetch": "^2.2.0",
    "memoizee": "^0.3.9",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.0",
    "react-router": "^1.0.3",
    "redux": "^3.3.1",
    "redux-router": "^1.0.0-beta7",
    "reselect": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.3",
    "babel": "^5.8.35",
    "babel-core": "^5.8.35",
    "babel-loader": "^5.4.0",
    "babel-plugin-react-transform": "^1.1.1",
    "chai": "^3.5.0",
    "css-loader": "^0.22.0",
    "extract-text-webpack-plugin": "^0.8.2",
    "html-webpack-plugin": "^1.7.0",
    "isparta": "^4.0.0",
    "jsdom": "^6.5.1",
    "json-loader": "^0.5.4",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "mocha": "^2.4.5",
    "mocha-jsdom": "^1.1.0",
    "postcss-loader": "^0.6.0",
    "react-addons-test-utils": "^0.14.7",
    "react-transform-catch-errors": "^1.0.2",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "redux-devtools": "^2.1.5",
    "rimraf": "^2.5.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "stats-webpack-plugin": "^0.2.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-hot-middleware": "^2.7.1"
  }
}
